{
  "name": "denosyscore/http",
  "description": "HTTP kernel, middleware, and exception handling",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "laminas/laminas-diactoros": "^3.6",
    "laminas/laminas-httphandlerrunner": "^2.12",
    "nyholm/psr7": "^1.8",
    "psr/event-dispatcher": "^1.0",
    "denosyscore/contracts": "^0.2",
    "denosyscore/container": "^0.2",
    "denosyscore/events": "^0.2",
    "denosyscore/validation": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "Denosys\\\\Http\\\\": "src/Http/",
      "Denosys\\\\Http\\\\Exceptions\\\\": "src/Exceptions/",
      "Denosys\\\\Http\\\\Security\\\\": "src/Security/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
